{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.bingchat4urapp_server.bingchat4urapp_server.Bingchat4urappServerApplication",
            "projectName": "bingchat4urapp_server",
            "vmArgs": "--add-exports java.base/java.lang=ALL-UNNAMED --add-exports java.desktop/sun.awt=ALL-UNNAMED --add-exports java.desktop/sun.java2d=ALL-UNNAMED",
            "args": "--proxy 127.0.0.1:2080 --examMode true --hideBrowser false"
        }
    ]
}